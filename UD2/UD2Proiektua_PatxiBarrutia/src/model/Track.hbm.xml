<hibernate-mapping schema="chinook">
 <class entity-name="model.Track" name="model.Track" table="track">
  <id column="TrackId" name="TrackId"/>
  <property column="Name" name="Name"/>
  <property column="AlbumId" name="AlbumId"/>
  <property column="MediaTypeId" name="MediaTypeId"/>
  <property column="GenreId" name="GenreId"/>
  <property column="Composer" name="Composer"/>
  <property column="Milliseconds" name="Milliseconds"/>
  <property column="Bytes" name="Bytes"/>
  <property column="UnitPrice" name="UnitPrice"/>
 </class>
</hibernate-mapping>
